<Window x:Class="Intervallo.Form.LicenseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p="clr-namespace:Intervallo.Properties"
        xmlns:local="clr-namespace:Intervallo.Form"
        mc:Ignorable="d"
        Title="LicenseWindow" Height="449" Width="695" WindowStartupLocation="CenterScreen" Icon="/Intervallo;component/icon.ico" ShowInTaskbar="False">
    <Grid>
        <TextBox x:Name="LicenseTextBox" Margin="10,10,10,35" TextWrapping="Wrap" IsReadOnly="True" AcceptsReturn="True" VerticalScrollBarVisibility="Visible"/>
        <Button Content="{x:Static p:LangResources.LicenseWindow_OK}" Margin="0,0,10,10" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
